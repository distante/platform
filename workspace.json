{
    "version": 2,
    "projects": {
        "api": "apps/api",
        "clampjs": "packages/clampjs",
        "conways-game-of-life": "apps/conways-game-of-life",
        "make-buildable": "packages/make-buildable",
        "nest-spectator": "packages/nest-spectator",
        "ngrx-data-websocket-client": "packages/ngrx-data-websocket/client",
        "ngrx-data-websocket-core": "packages/ngrx-data-websocket/core",
        "ngrx-data-websocket-demo": "apps/ngrx-data-websocket-demo",
        "ngrx-data-websocket-server": "packages/ngrx-data-websocket/server",
        "ngrx-universal-rehydrate": "packages/ngrx-universal-rehydrate",
        "ngrx-universal-rehydrate-demo": "apps/ngrx-universal-rehydrate-demo",
        "ngrx-universal-rehydrate-demo-api": "apps/ngrx-universal-rehydrate-demo-api",
        "nxls": "packages/nxls",
        "rx-dynamic-component": "packages/rx-dynamic-component",
        "rx-dynamic-component-demo": "apps/rx-dynamic-component-demo"
    },
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "generators": {
        "@nrwl/workspace": {
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/cypress": {
            "cypress-project": {
                "linter": "eslint"
            }
        },
        "@nrwl/react": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/next": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/web": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/node": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/nx-plugin": {
            "plugin": {
                "linter": "eslint"
            }
        },
        "@nrwl/nest": {
            "application": {
                "linter": "eslint"
            }
        },
        "@nrwl/express": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "defaultProject": "ngrx-universal-rehydrate-demo-api"
}
